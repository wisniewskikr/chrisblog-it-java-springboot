type Query {
    view(id: ID!): UserDto,
    list: [UserDto]
}

type Mutation {
    create(command: CreateCommand!): UserDto,
    edit(id: ID!, command: EditCommand!): UserDto
}

type UserDto {
    id: ID!
    name: String!
}

input CreateCommand {
    name: String!
}

input EditCommand {
    name: String!
}